# Ranges for sensitivity analysis
const.beta: [0.1, 10.0]
const.PM.delta_threshold: [0.1, 1.0]
const.HM.delta_threshold: [0.01, 1.0]
const.EM.delta_threshold: [0.01, 0.1]
const.gamma: [0, 10]
init.HM.threshold_0: [0.0, -0.9]
