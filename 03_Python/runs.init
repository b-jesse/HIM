GOV.res_subsidy: 0
GOV.power_subsidy: 0
GOV.power_guarant: 0
GOV.elc_subsidy: 0
GOV.elc_guarant: 0
GOV.man_subsidy: 0

# Ranges for sensitivity analysis
const.beta: [0.1, 2]
const.PM.delta_threshold: [0.01, 0.1]
const.HM.delta_threshold: [0.01, 0.1]
const.EM.delta_threshold: [0.01, 0.1]
const.gamma: [0.1, 1]
init.HM.threshold_0: [0.1, 0.9]
