# Model settings
plot: false
write: true
debug: false
track: false

# Scenario settings
ref: true
co2_tax: false
h2_subsidy: false
h2_guarant: false
res_subsidy: false
power_subsidy: false
power_guarant: false
elc_subsidy: false
elc_guarant: false
man_subsidy: false
